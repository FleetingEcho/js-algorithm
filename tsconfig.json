{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"allowJs": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"strictNullChecks": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitThis": false,
		"moduleResolution": "Node",
		"forceConsistentCasingInFileNames": true
	}
}
